{
    "name": "Waste Meter",
    "version": "0.1",

    "permissions": [
	"tabs",
	"http://*/*",
	"storage"
    ],

    "icons": { "64": "icon.png" },
    
    "browser_action": {
	"default_title": "test",
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },
    
    "content_scripts": [
	{
	    "matches": ["http://*/*"],
	    "js": ["jquery-1.11.2.min.js", "script.js"],
	    "run_at": "document_start"
	}
    ],
    
    "manifest_version": 2,

    "description": "Measure load time for pages"
}
